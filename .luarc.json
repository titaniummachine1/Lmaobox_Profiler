{
    "$schema": "https://raw.githubusercontent.com/LuaLS/vscode-lua/master/setting/schema.json",
    "Lua.runtime.version": "Lua 5.4",
    "Lua.runtime.special": {
        "require": "require"
    },
    "Lua.diagnostics.globals": [
        "_G",
        "collectgarbage",
        "debug",
        "string",
        "table",
        "math",
        "io",
        "os",
        "package",
        "draw",
        "globals",
        "input",
        "callbacks",
        "entities",
        "engine", 
        "client",
        "KEY_P",
        "MOUSE_LEFT",
        "MOUSE_WHEEL_UP", 
        "MOUSE_WHEEL_DOWN",
        "IN_FORWARD",
        "IN_BACK", 
        "IN_MOVELEFT",
        "IN_MOVERIGHT",
        "IN_ATTACK",
        "IN_ATTACK2",
        "FL_ONGROUND"
    ],
    "Lua.diagnostics.disable": [
        "unused-local",
        "unused-vararg",
        "lowercase-global",
        "global-in-nil-env",
        "undefined-field",
        "assign-type-mismatch",
        "need-check-nil",
        "inject-field"
    ],
    "Lua.workspace.library": [],
    "Lua.workspace.checkThirdParty": false,
    "Lua.completion.enable": true,
    "Lua.completion.callSnippet": "Both",
    "Lua.completion.keywordSnippet": "Both",
    "Lua.completion.displayContext": 3,
    "Lua.hover.enable": true,
    "Lua.hover.viewString": true,
    "Lua.hover.viewStringMax": 1000,
    "Lua.hover.viewNumber": true,
    "Lua.hover.fieldInfer": 1000,
    "Lua.hover.previewFields": 50,
    "Lua.hover.enumsLimit": 5,
    "Lua.hover.expandAlias": true,
    "Lua.signatureHelp.enable": true,
    "Lua.hint.enable": true,
    "Lua.hint.paramType": true,
    "Lua.hint.setType": false,
    "Lua.hint.paramName": "All",
    "Lua.hint.semicolon": "All",
    "Lua.hint.arrayIndex": "Auto",
    "Lua.format.enable": true,
    "Lua.format.defaultConfig": {
        "indent_style": "tab",
        "indent_size": "1",
        "continuation_indent_size": "1",
        "max_line_length": "120",
        "end_of_line": "crlf",
        "insert_final_newline": "true",
        "quote_style": "double",
        "call_arg_parentheses": "keep",
        "detect_end_of_line": "false",
        "trailing_table_separator": "smart",
        "space_around_table_field_list": "true",
        "space_before_attribute": "true",
        "space_after_attribute": "true",
        "space_before_function_open_parenthesis": "false",
        "space_before_function_call_open_parenthesis": "false",
        "space_before_closure_open_parenthesis": "false",
        "space_before_function_call_single_arg": "true",
        "space_before_open_square_bracket": "false",
        "space_inside_function_call_parentheses": "false",
        "space_inside_function_param_list_parentheses": "false",
        "space_inside_square_brackets": "false",
        "space_around_math_operator": "true",
        "space_after_comma": "true",
        "space_after_comma_in_for_statement": "true",
        "space_around_concat_operator": "true",
        "align_call_args": "false",
        "align_function_params": "true",
        "align_continuous_assign_statement": "false",
        "align_continuous_rect_table_field": "true",
        "align_array_table": "true",
        "never_indent_before_if_condition": "false",
        "never_indent_comment_on_if_branch": "false",
        "keep_indents_on_empty_lines": "false",
        "allow_non_indented_comments": "false"
    },
    "Lua.semantic.enable": true,
    "Lua.semantic.variable": true,
    "Lua.semantic.annotation": true,
    "Lua.semantic.keyword": false,
    "Lua.IntelliSense.traceLocalSet": false,
    "Lua.IntelliSense.traceReturn": true,
    "Lua.IntelliSense.traceBeSetted": true,
    "Lua.IntelliSense.traceFieldInject": true,
    "Lua.window.statusBar": true,
    "Lua.window.progressBar": true,
    "Lua.misc.parameters": [],
    "Lua.misc.executablePath": "",
    "Lua.telemetry.enable": false,
    "Lua.spell.dict": []
}
